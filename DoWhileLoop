//Do while Loop

//Program 1
fun main(args: Array<String>){
    var i:Int = 1
    do{
        println(i)
        i++
    }while(i <= 10)
}

//Program 2
fun main(args: Array<String>){
    var i:Int = 1
    do{
        if(i % 2 == 0){
            println(i)
        }
        i++
    }while(i <= 10)
}
