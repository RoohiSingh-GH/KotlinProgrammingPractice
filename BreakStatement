//Break Statement

//Program 1
fun main(args: Array<String>){
    for (i in 1..10){
        println(i)
        if (i == 5){
            break
        }
    }
}

//Program 2 (Labeled For Loop with break statement)
fun main(args: Array<String>){
   myLoop@ for (i in 1..3){
        for (j in 1..3){
            println("$i $j")
            if(i == 2 && j == 2){
                break@myLoop
            }
        }
    }
}
